<tr>
  <td class="post">
    <span class="content"><%= post.content %></span>
    <span class="timestamp">
      Posted <%= time_ago_in_words(post.create_time) %> ago.
    </span>
    <span>205 comments <%= link_to 'Show ', post %><%= link_to 'Edit ', edit_post_path(post) %><%= link_to 'Destroy', post, method: :delete, data: {confirm: 'Are you sure?'} %> </span>
  </td>
</tr>